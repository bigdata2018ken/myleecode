        # è¾“å‡ºæ ˆä¸ºç©º,å°†è¾“å…¥æ ˆçš„å…ƒç´ å‹å…¥è¾“å‡ºæ ˆ
        else:
            while self.inStack:
                val = self.inStack.pop()
                self.outStack.append(val)
            return self.outStack.pop()

    def peek(self) -> int:
        """
        Get the front element.
        """
        # ä½¿ç”¨å·²æœ‰çš„å‡½æ•° pop
        res = self.pop()
        # pop å‡½æ•°å¼¹å‡ºäº† resï¼Œæ‰€ä»¥è¦å†æ·»åŠ å›å»
        self.outStack.append(res)

        return res


    def empty(self) -> bool:
        """
        Returns whether the queue is empty.
        """
        # ä¸¤ä¸ªæ ˆéƒ½ä¸ºç©ºï¼Œé˜Ÿåˆ—æ‰ä¸ºç©º
        if not(self.inStack or self.outStack):
            return True

        return False



