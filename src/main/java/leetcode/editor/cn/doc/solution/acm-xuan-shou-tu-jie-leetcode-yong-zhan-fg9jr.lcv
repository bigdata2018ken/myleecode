class MyQueue:

    def __init__(self):
        """
        Initialize your data structure here.
        """
        # åˆå§‹åŒ–è¾“å…¥æ ˆå’Œè¾“å‡ºæ ˆ
        self.inStack = []
        self.outStack = []

    def push(self, x: int) -> None:
        """
        Push element x to the back of queue.
        """
        # æœ‰æ–°å…ƒç´ è¿›æ¥ï¼Œè¿›å…¥è¾“å…¥æ ˆ
        self.inStack.append(x)

    def pop(self) -> int:
        """
        Removes the element from in front of queue and returns that element.
        """
        # å¦‚æœä¸ºç©º
        if self.empty():
            return None

        # å¦‚æœè¾“å‡ºæ ˆä¸ä¸ºç©ºï¼Œè¿”å›è¾“å‡ºæ ˆä¸­çš„å…ƒç´ 
        if self.outStack:
